{
	"name": "Iain's Theme",
	"type": "dark",
	"colors": {
		"editor.background": "#1a1d24",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable",
				"entity.name.variable",
				"variable.other",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#ffd27e" // #f07178 // #fce6bb
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other",
				"storage.type",
				"storage.modifier",
				"storage.function"
			],
			"settings": {
				"foreground": "#e84279"
			}
		},
		{
			"name": "Keyword Operators",
			"scope": [
				"keyword.operator",
